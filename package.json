  "version": "1.0.0",
  "license": "MIT",
  "author": "Oleksandr Aleksieiev",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:aleksieiev-o/portfolio-admin-panel.git"
  },
  "bugs": {
    "url": "https://github.com/aleksieiev-o/portfolio-admin-panel/issues"
  },
  "scripts": {
    "dev": "next dev",
		"fb_em_start": "firebase emulators:start",
		"fb_em_import": "firebase emulators:start --import ./src/lib/firebase/seed.development",
		"fb_em_export": "firebase emulators:export ./src/lib/firebase/seed.development",
    "fb_em": "firebase emulators:start --import ./src/lib/firebase/seed.development --export-on-exit ./src/lib/firebase/seed.development",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint_fix": "next lint --fix",
		"inc ver patch": "./bump-version.sh patch",
		"inc ver minor": "./bump-version.sh minor",
		"inc ver major": "./bump-version.sh major",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@chakra-ui/theme-tools": "^2.0.17",
    "@mui/icons-material": "^5.14.13",
    "@types/node": "18.17.11",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "firebase": "^10.5.0",
    "firebase-admin": "^12.1.1",
    "formik": "^2.4.5",
    "my-portfolio-types": "^1.0.0",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-icons": "^4.11.0",
    "sass": "^1.69.3",
    "typescript": "5.2.2",
    "yup": "^1.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "husky": "^8.0.3"
  }
}
